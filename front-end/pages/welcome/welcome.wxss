.container{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.avatar{
  width: 200rpx;
  height: 200rpx;
  margin-top: 160rpx;
}
.motto{
  margin-top: 100rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: #FFFFFF;
}
.form{
  margin-top: 50rpx
}
.row-wrap{
  margin-top: 20rpx;
  margin-left: 100rpx;
  margin-right: 100rpx;
  display:flex;
  flex-direction: row;
  border-bottom: 1px solid #E0E0E0;
}
.label{
  margin-right: 30rpx;
  width: 250rpx;
  font-size:30rpx;
  color: #FFFFFF
}
.choice{
